<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.suixing.mapper.CarMapper">
    <!-- 举例   -->
    <select id="selectByCarId" resultType="com.suixing.entity.Car">
        select * from car where car_id =#{carId}
    </select>

    <select id="getBussiness" resultType="com.suixing.entity.Car">
        select bus_name,bus_address from car car,business bussiness where car.bus_id=business.bus_id =#{busId}
    </select>

</mapper>
